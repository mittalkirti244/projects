{
    "_version": "1.32.0",
    "sap.app": {
        "id": "mrorequestdolphinui",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/mrorequestdolphin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.98.0",
            "libs": {
                "sap.ui.core": {},
                "sap.fe.templates": {}
            }
        },
        "models": {
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "MaintenanceRequestsList",
                    "target": "MaintenanceRequestsList"
                },
                {
                    "pattern": "MaintenanceRequests({key}):?query:",
                    "name": "MaintenanceRequestsObjectPage",
                    "target": "MaintenanceRequestsObjectPage"
                },
                {
                    "pattern": "MaintenanceRequests({key})/to_document({key2}):?query:",
                    "name": "DocumentsObjectPage",
                    "target": "DocumentsObjectPage"
                }
            ],
            "targets": {
                "MaintenanceRequestsList": {
                    "type": "Component",
                    "id": "MaintenanceRequestsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "MaintenanceRequests",
                            "variantManagement": "Page",
                            "navigation": {
                                "MaintenanceRequests": {
                                    "detail": {
                                        "route": "MaintenanceRequestsObjectPage"
                                    }
                                }
                            },
                            "initialLoad": "Enabled",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "selectAll": false
                                    }
                                }
                            }
                        }
                    }
                },
                "MaintenanceRequestsObjectPage": {
                    "type": "Component",
                    "id": "MaintenanceRequestsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "content": {
                                "header": {
                                    "actions": {
                                        "MenuActions": {
                                            "text": "My Menu Button",
                                            "menu": [
                                                "DataFieldForAction::mrorequestdolphinService.readyForWorkListRequested",
                                                "DataFieldForAction::mrorequestdolphinService.workListRequested",
                                                "DataFieldForAction::mrorequestdolphinService.newWorkListReceived",
                                                "DataFieldForAction::mrorequestdolphinService.workListValidated",
                                                "DataFieldForAction::mrorequestdolphinService.workListUploaded",
                                                "DataFieldForAction::mrorequestdolphinService.allWorkListReceived",
                                                "DataFieldForAction::mrorequestdolphinService.revisionCreated",
                                                "DataFieldForAction::mrorequestdolphinService.allTaskListIdentified",
                                                "DataFieldForAction::mrorequestdolphinService.allNotificationCreated",
                                                "DataFieldForAction::mrorequestdolphinService.mrReadyForApproval",
                                                "DataFieldForAction::mrorequestdolphinService.requestedApproved"
                                            ]
                                        }
                                    }
                                }
                            },
                            "editableHeaderContent": false,
                            "entitySet": "MaintenanceRequests",
                            "controlConfiguration": {
                                "to_document/@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "personalization": {
                                            "column": true,
                                            "sort": false
                                        },
                                        "multiSelect": true,
                                        "selectAll": false,
                                        "enablePaste": false,
                                        "enableExport": true,
                                        "enableSettings": false
                                    }
                                }
                            },
                            "navigation": {
                                "to_document": {
                                    "detail": {
                                        "route": "DocumentsObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "DocumentsObjectPage": {
                    "type": "Component",
                    "id": "DocumentsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "Documents",
                            "navigation": {},
                            "sectionLayout": "Page",
                            "editableHeaderContent": false
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}